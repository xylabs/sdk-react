{
  "files.exclude": {
    "**/node_modules": false,
    "**/*.tsconfig.*": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/.yarn": true,
    "**/yarn.lock": true,
    "**/build": true,
    "**/dist": true,
  },
  "editor.formatOnSave": true,
  "editor.quickSuggestions": {
    "comments": "on",
    "strings": "on",
    "other": "on"
  },
  "editor.tabSize": 2,
  "editor.suggestSelection": "first",
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "never"
  },
  "eslint.alwaysShowStatus": true,
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "files.autoSave": "onFocusChange",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[typescriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "cSpell.words": [
    "adword",
    "Adwords",
    "appbar",
    "arbitrum",
    "Arie",
    "arietrouw",
    "autodocs",
    "awid",
    "bignumber",
    "bowser",
    "darkmode",
    "Dataism",
    "deepmerge",
    "docgen",
    "ethersproject",
    "flexbox",
    "Geospatial",
    "Inpage",
    "instrumenter",
    "jsdelivr",
    "Merkle",
    "noindex",
    "Overrided",
    "pageview",
    "Perma",
    "Promisable",
    "rdns",
    "reinit",
    "rfdc",
    "Rollbar",
    "Tapjoy",
    "Tiktok",
    "Trouw",
    "typedoc",
    "viem",
    "wagmi",
    "webextension",
    "webp",
    "xylabs",
    "Xyos",
    "yarnpkg"
  ]
}
