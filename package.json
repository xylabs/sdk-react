{
  "name": "@xylabs/sdk-react",
  "author": {
    "email": "support@xylabs.com",
    "name": "XY Labs Development Team",
    "url": "https://xylabs.com"
  },
  "bugs": {
    "email": "support@xylabs.com",
    "url": "https://github.com/xylabs/sdk-react/issues"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.8.2",
    "@xylabs/react-button": "workspace:^",
    "@xylabs/react-common": "workspace:^",
    "@xylabs/react-cookie-consent": "workspace:^",
    "@xylabs/react-crypto": "workspace:^",
    "@xylabs/react-experiments": "workspace:^",
    "@xylabs/react-flexbox": "workspace:^",
    "@xylabs/react-identicon": "workspace:^",
    "@xylabs/react-pixel": "workspace:^",
    "@xylabs/react-pixel-debugger": "workspace:^",
    "@xylabs/react-shared": "workspace:^"
  },
  "description": "Common React library for all XY Labs projects that use React",
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@storybook/addon-actions": "^6.5.6",
    "@storybook/addon-docs": "^6.5.6",
    "@storybook/addon-essentials": "^6.5.6",
    "@storybook/addon-interactions": "^6.5.6",
    "@storybook/addon-links": "^6.5.6",
    "@storybook/addons": "^6.5.6",
    "@storybook/api": "^6.5.6",
    "@storybook/builder-webpack5": "^6.5.6",
    "@storybook/components": "^6.5.6",
    "@storybook/core-events": "^6.5.6",
    "@storybook/manager-webpack5": "^6.5.6",
    "@storybook/react": "^6.5.6",
    "@storybook/testing-library": "^0.0.11",
    "@storybook/theming": "^6.5.6",
    "@types/react": "^18.0.10",
    "@xylabs/eslint-config-react": "^2.1.5",
    "@xylabs/ts-scripts": "^1.0.66",
    "@xylabs/tsconfig": "^1.0.13",
    "copyfiles": "^2.4.1",
    "depcheck": "^1.4.3",
    "eslint": "^8.17.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "rimraf": "^3.0.2",
    "storybook-dark-mode": "^1.1.0",
    "ts-node": "^10.8.1",
    "typedoc": "^0.22.17",
    "typescript": "^4.7.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "resolutions": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "browser": "dist/esm/index.js",
  "docs": "dist/docs.json",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "browser": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./dist/docs.json": {
      "default": "./dist/docs.json"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "homepage": "https://xylabs.com",
  "keywords": [
    "utility",
    "typescript",
    "react"
  ],
  "license": "LGPL-3.0",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xylabs/sdk-react.git"
  },
  "scripts": {
    "build-storybook": "build-storybook",
    "deploy": "yarn version patch -i && yarn build && yarn npm publish",
    "lint-pkg": "npmPkgJsonLint .",
    "start": "start-storybook -p 6006",
    "copy-images:cjs": "yarn workspaces foreach -ptRv run copy-images:package:cjs",
    "copy-images:esm": "yarn workspaces foreach -ptRv run copy-images:package:esm",
    "test": "env TS_NODE_PROJECT=\"tsconfig.test.json\" mocha -r ts-node/register 'src/**/*.spec.ts'",
    "deploy3": "yarn deploy-patch",
    "deploy-major": "yarn workspaces foreach --all version major --deferred && yarn deploy-common",
    "deploy-minor": "yarn workspaces foreach --all version minor --deferred && yarn deploy-common",
    "deploy-patch": "yarn workspaces foreach --all version patch --deferred && yarn deploy-common",
    "deploy-common": "yarn build && yarn gen:docs && yarn version apply --all && yarn workspaces foreach -ptv npm publish",
    "clean": "yarn workspaces foreach -pv run clean:package",
    "deps": "yarn workspaces foreach -pv run deps:package",
    "lint": "yarn workspaces foreach -pv run lint:package",
    "fix": "yarn workspaces foreach -pv run fix:package",
    "outdated": "yarn workspaces foreach -pv outdated",
    "up": "yarn workspaces foreach -pv up",
    "updo": "yarn workspaces foreach -pv run updo3:package",
    "build": "yarn clean && yarn build:esm && yarn build:cjs && yarn copy-images:esm && yarn copy-images:cjs && yarn gen:docs",
    "build:cjs": "yarn workspaces foreach -ptRv run build:package:cjs",
    "build:esm": "yarn workspaces foreach -ptRv run build:package:esm",
    "gen:docs": "typedoc ./src/index.ts --json ./dist/docs.json --tsconfig ./tsconfig.json",
    "gen:docs:package": "typedoc $INIT_CWD/src/index.ts --json $INIT_CWD/dist/docs.json",
    "clean:package": "rimraf -q $INIT_CWD/dist && rimraf -q $INIT_CWD/build",
    "fix:package": "yarn eslint $INIT_CWD/src --fix",
    "lint:package": "yarn eslint $INIT_CWD/src",
    "deps:package": "depcheck $INIT_CWD",
    "updo3:package": "yarn update-interactive",
    "build:package:cjs": "tsc -p $INIT_CWD/tsconfig.build.cjs.json",
    "build:package:esm": "tsc -p $INIT_CWD/tsconfig.build.esm.json",
    "copy-images:package:esm": "copyfiles -u 9 \"$INIT_CWD/src/**/*.svg\" \"$INIT_CWD/src/**/*.jpg\" \"$INIT_CWD/src/**/*.gif\" \"$INIT_CWD/src/**/*.png\" $INIT_CWD/dist/esm",
    "copy-images:package:cjs": "copyfiles -u 9 \"$INIT_CWD/src/**/*.svg\" \"$INIT_CWD/src/**/*.jpg\" \"$INIT_CWD/src/**/*.gif\" \"$INIT_CWD/src/**/*.png\" $INIT_CWD/dist/cjs"
  },
  "sideEffects": false,
  "types": "dist/esm/index.d.ts",
  "version": "2.13.4",
  "packageManager": "yarn@3.1.1"
}
